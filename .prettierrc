{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "always",
  "singleAttributePerLine": true,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "index.html",
      "options": {
        "singleAttributePerLine": false
      }
    }
  ]
}
