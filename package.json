{
  "name": "neos",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "scripts": {
    "ng": "ng",
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:coverage": "ng test --watch=false --code-coverage",
    "lint:check": "ng lint",
    "lint:fix": "ng lint --fix",
    "format:check": "prettier **/*.{js,json,ts,html,css,scss,md} --check",
    "format:fix": "prettier **/*.{js,json,ts,html,css,scss,md} --write",
    "clean": "rm -rf dist tests",
    "clean:lock": "npm run clean && rm -rf package-lock.json",
    "clean:force": "npm run clean:lock && rm -rf node_modules && npm cache clean --force",
    "ci:lint": "npm run format:check && npm run lint:check",
    "ci:test": "ng test --watch=false --code-coverage --browsers ChromeHeadless",
    "ci:build": "ng build",
    "ci:build:uat": "ng build --configuration uat",
    "ci:build:production": "ng build --configuration production",
    "ci:name": "node -pe \"require('./package.json').name\"",
    "ci:version": "node -pe \"require('./package.json').version\"",
    "ci:build:dynamicForm": "ng build dynamic-form --configuration production",
    "ci:build:tableGenerator": "ng build table-generator --configuration production",
    "ci:build:loading": "ng build loading --configuration production",
    "ci:pack": "npm pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.4",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.2.4",
    "@angular/compiler": "~11.2.4",
    "@angular/core": "~11.2.4",
    "@angular/forms": "~11.2.4",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.2.4",
    "@angular/platform-browser-dynamic": "~11.2.4",
    "@angular/router": "~11.2.4",
    "@mat-datetimepicker/core": "^6.0.3",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0",
    "mat-timepicker": "^4.1.1",
    "morgan": "^1.10.0",
    "ngx-logger": "^4.3.3",
    "nodemon": "^3.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.3",
    "@angular-eslint/builder": "4.3.1",
    "@angular-eslint/eslint-plugin": "4.3.1",
    "@angular-eslint/eslint-plugin-template": "4.3.1",
    "@angular-eslint/schematics": "4.3.1",
    "@angular-eslint/template-parser": "4.3.1",
    "@angular/cli": "~11.2.3",
    "@angular/compiler-cli": "~11.2.4",
    "@angular/localize": "^11.2.14",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.20.55",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.12",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "ng-packagr": "^11.0.0",
    "ngx-translate-testing": "^5.1.0",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "ts-node": "~8.3.0",
    "typescript": "~4.1.5"
  }
}
